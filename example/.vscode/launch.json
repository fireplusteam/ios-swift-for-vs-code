{
    "version": "0.2.0",
    "configurations": [
        { // this is for testing python files
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["698B2C6D-4FA9-4142-B802-C9DF3C41E049","puzzle.TestVSCode"],
            "justMyCode": true,
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "attach",
            "name": "Debug iOS App",
            "pid": "${config:iOS_PID}",
            "stopOnEntry": false
        },
        {
            "name": "Show iOS App Log",
            "type": "node",
            "request": "launch",
            "program": ".vscode/l.js",
            "stopOnEntry": false,
            "args": [
                ".vscode/app.log",
            ],
            "console": "internalConsole",
            "internalConsoleOptions": "neverOpen",
            "envFile": "${workspaceFolder}/.vscode/.env",
            "cwd": "${workspaceFolder}"
        }
    ]
}